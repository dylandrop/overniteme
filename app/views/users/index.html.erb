<%= will_paginate @users %>
<table class="table table-striped">
  <% @users.each do |user| %>
    <tr>
      <td><%= link_to user.username, user %></td>
      <td><%= user.description %></td>
      <td><%= link_to "Message #{user.username}", new_conversation_path(user: user) %></td>
    </tr>
  <% end %>
</table>